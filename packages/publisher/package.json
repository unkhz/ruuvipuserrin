{
  "name": "@ruuvipuserrin/publisher",
  "version": "0.0.0",
  "type": "commonjs",
  "module": "index.ts",
  "scripts": {
    "prettier-write": "prettier --write *.json *.ts lib/**/*.ts",
    "prettier-check": "prettier --check *.json *.ts lib/**/*.ts",
    "test-unit": "jest --no-cache --ci",
    "lint": "npm run prettier-check",
    "test": "npm run test-unit",
    "build": "tsc --project tsconfig.build.json",
    "start": "ts-node index.ts --pollingInterval=15000 --taskQueue=ruuvipuserrin"
  },
  "devDependencies": {
    "@ruuvipuserrin/base-node": "*"
  },
  "dependencies": {
    "@temporalio/client": "^1.6.0",
    "@types/minimist": "",
    "minimist": "^1.2.7",
    "zod": "^3.20.2"
  }
}
