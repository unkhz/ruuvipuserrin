{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "commonjs",
    "target": "esnext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "types": ["node"],
    "outDir": "./dist"
  }
}
